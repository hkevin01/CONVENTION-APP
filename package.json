    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.0.7",
    "concurrently": "9.1.2",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
